{
  "rootDir": "../../",
  "verbose": false,
  "testURL": "http://wpshopify.loc",
  "modulePathIgnorePatterns": [
    "<rootDir>/node_modules",
    "<rootDir>/classes",
    "<rootDir>/vendor",
    "<rootDir>/.git"
  ],
  "roots": [
    "<rootDir>/admin/js/app",
    "<rootDir>/public/js/app"
  ],
  "notify": true,
  "setupFiles": [
    "<rootDir>/jest-test-env.js"
  ],
  "setupFilesAfterEnv": ["<rootDir>/jest-test-setup.js"],
  "testRegex": "integration.test.js"
}
